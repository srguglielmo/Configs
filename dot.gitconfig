[user]
	name = Stephen Guglielmo
	email = srg@
[push]
	default = nothing
[core]
	editor = vim
	autocrlf = input
	whitespace = blank-at-eol,space-before-tab,indent-with-non-tab,blank-at-eof,tabwidth=4
	safecrlf = true
	compression = 6
[merge]
	tool = vimdiff
[status]
	branch = true
[column]
	ui = auto,row
	#branch = auto
	#clean = auto
	#status = auto
	#tag = auto
[fetch]
	fsckObjects = true
[receive]
	fsckObjects = true
[transfer]
	fsckObjects = true
